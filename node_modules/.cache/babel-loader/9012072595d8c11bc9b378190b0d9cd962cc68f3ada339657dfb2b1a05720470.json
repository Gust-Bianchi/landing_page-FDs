splay:flex;\r\n  gap: 12px;\r\n  margin-bottom: 18px;\r\n}\r\n\r\n.btn-primary{\r\n  padding: 14px 18px;\r\n  border-radius: 999px;\r\n  background: var(--blue);\r\n  color:#fff;\r\n  font-weight: 950;\r\n  text-decoration:none;\r\n}\r\n\r\n.btn-ghost{\r\n  padding: 14px 18px;\r\n  border-radius: 999px;\r\n  border: 2px solid rgba(31,95,145,0.28);\r\n  color: var(--blue);\r\n  text-decoration:none;\r\n  font-weight: 950;\r\n}\r\n\r\n.metrics{\r\n  display:grid;\r\n  grid-template-columns: repeat(3,1fr);\r\n  gap: 12px;\r\n}\r\n\r\n.metric{\r\n  padding: 14px;\r\n  border-radius: 18px;\r\n}\r\n\r\n.kpi{ font-size:1.35rem; font-weight:950; }\r\n.label{ font-weight:900; opacity:.75; }\r\n.desc{ font-size:.92rem; opacity:.8; }\r\n\r\n/* FORM */\r\n.panel{\r\n  padding: 22px;\r\n  border-radius: 24px;\r\n}\r\n\r\n.panelHead{\r\n  display:flex;\r\n  justify-content: space-between;\r\n  margin-bottom: 14px;\r\n}\r\n\r\n.form{ display:flex; flex-direction: column; gap: 12px; }\r\n\r\n.row{ display:flex; flex-direction: column; gap: 7px; }\r\n\r\ninput{\r\n  height: 48px;\r\n  border-radius: 14px;\r\n  border: 1px solid rgba(31,95,145,0.22);\r\n  padding: 0 14px;\r\n  background: rgba(255,255,255,0.9);\r\n}\r\n\r\n.submit{\r\n  height: 50px;\r\n  border-radius: 14px;\r\n  background: var(--blue);\r\n  color:#fff;\r\n  font-weight:950;\r\n  border:none;\r\n}\r\n\r\n.error{\r\n  color: var(--blue);\r\n  font-weight: 900;\r\n}\r\n\r\n.hint{\r\n  font-size:.9rem;\r\n  opacity:.7;\r\n}\r\n\r\n@media (max-width: 980px){\r\n  .wrap{ grid-template-columns: 1fr; }\r\n  .metrics{ grid-template-columns: 1fr; }\r\n}\r\n</style>"],"mappings":";;;AA8HA,IAAMA,UAAS,GAAI,SAAbA,UAASA,CAAIC,CAAA;EAAA,OAAK,CAACA,CAAA,IAAK,EAAE,EAAEC,OAAO,CAAC,KAAK,EAAE,EAAE,CAAC;AAAA;AAEpD,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAI,WAAJA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,GAAG,EAAE,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACT,CAAC;MACDC,KAAK,EAAE;IACT,CAAC;EACH,CAAC;EACDC,OAAO,EAA