{"ast":null,"code":"import \"core-js/modules/es.array.slice.js\";\nimport \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.replace.js\";\nvar onlyDigits = function onlyDigits(v) {\n  return (v || \"\").replace(/\\D/g, \"\");\n};\nexport default {\n  name: \"HeroSection\",\n  data: function data() {\n    return {\n      form: {\n        nome: \"\",\n        cpf: \"\",\n        cnpj: \"\",\n        email: \"\"\n      },\n      error: \"\"\n    };\n  },\n  methods: {\n    maskCpf: function maskCpf(v) {\n      var d = onlyDigits(v).slice(0, 11);\n      return d.replace(/^(\\d{3})(\\d)/, \"$1.$2\").replace(/^(\\d{3})\\.(\\d{3})(\\d)/, \"$1.$2.$3\").replace(/^(\\d{3})\\.(\\d{3})\\.(\\d{3})(\\d{1,2})$/, \"$1.$2.$3-$4\");\n    },\n    maskCnpj: function maskCnpj(v) {\n      var d = onlyDigits(v).slice(0, 14);\n      return d.replace(/^(\\d{2})(\\d)/, \"$1.$2\").replace(/^(\\d{2})\\.(\\d{3})(\\d)/, \"$1.$2.$3\").replace(/^(\\d{2})\\.(\\d{3})\\.(\\d{3})(\\d)/, \"$1.$2.$3/$4\").replace(/^(\\d{2})\\.(\\d{3})\\.(\\d{3})\\/(\\d{4})(\\d{1,2})$/, \"$1.$2.$3/$4-$5\");\n    },\n    onSubmit: function onSubmit() {\n      var cpfDigits = onlyDigits(this.form.cpf);\n      var cnpjDigits = onlyDigits(this.form.cnpj);\n      if (cpfDigits.length !== 11) {\n        this.error = \"CPF inválido. Preencha corretamente.\";\n        return;\n      }\n      if (cnpjDigits.length !== 14) {\n        this.error = \"CNPJ inválido. Preencha corretamente.\";\n        return;\n      }\n      this.error = \"\";\n      alert(\"Dados enviados com sucesso! Entraremos em contato.\");\n      this.form = {\n        nome: \"\",\n        cpf: \"\",\n        cnpj: \"\",\n        email: \"\"\n      };\n    }\n  }\n};","map":{"version":3,"names":["onlyDigits","v","replace","name","data","form","nome","cpf","cnpj","email","error","methods","maskCpf","d","slice","maskCnpj","onSubmit","cpfDigits","cnpjDigits","length","alert"],"sources":["C:\\Users\\prm\\Documents\\front-fields-main\\src\\components\\HeroSection.vue"],"sourcesContent":["<template>\r\n  <section class=\"hero\" id=\"inicio\">\r\n    <!-- Background tech -->\r\n    <div class=\"bg\" aria-hidden=\"true\">\r\n      <div class=\"glow g1\"></div>\r\n      <div class=\"glow g2\"></div>\r\n      <div class=\"grid\"></div>\r\n    </div>\r\n\r\n    <div class=\"container wrap\">\r\n      <!-- LEFT -->\r\n      <div class=\"left\">\r\n        <div class=\"pill glass\">\r\n          <span class=\"dot\"></span>\r\n          <span>Motor de decisão • Integração via API</span>\r\n        </div>\r\n\r\n        <h1>\r\n          Aprove mais crédito em <span class=\"accent\">segundos</span> com um\r\n          motor de decisão <span class=\"accent\">automático</span>.\r\n        </h1>\r\n\r\n        <p class=\"lead\">\r\n          Reduza a inadimplência em até <strong>30%</strong> com dados confiáveis,\r\n          modelos robustos de risco e rastreabilidade completa das decisões.\r\n        </p>\r\n\r\n        <div class=\"actions\">\r\n          <a class=\"btn-primary\" href=\"#contato\">Agende uma demonstração gratuita</a>\r\n          <a class=\"btn-ghost\" href=\"#produtos\">Ver produtos</a>\r\n        </div>\r\n\r\n        <div class=\"metrics\">\r\n          <div class=\"glass metric\">\r\n            <span class=\"label\">Análise em</span>\r\n\r\n            <span class=\"kpi accent\">10Segundos</span>\r\n            <span class=\"desc\">Decisões em tempo real</span>\r\n          </div>\r\n          <div class=\"glass metric\">\r\n            <span class=\"kpi accent\">+30%</span>\r\n            <span class=\"label\">Performance</span>\r\n            <span class=\"desc\">Menos inadimplência</span>\r\n          </div>\r\n          <div class=\"glass metric\">\r\n            <span class=\"kpi accent\">API</span>\r\n            <span class=\"label\">Integração</span>\r\n            <span class=\"desc\">Plug & play</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- RIGHT / FORM -->\r\n      <div class=\"right\" id=\"contato\">\r\n        <div class=\"glass panel glass-strong\">\r\n          <div class=\"panelHead\">\r\n            <div>\r\n              <h3>Agende uma demonstração</h3>\r\n              <p>Preencha os dados e nossa equipe entra em contato.</p>\r\n            </div>\r\n          </div>\r\n\r\n          <form class=\"form\" @submit.prevent=\"onSubmit\">\r\n            <div class=\"row\">\r\n              <label for=\"nome\">Nome</label>\r\n              <input\r\n                id=\"nome\"\r\n                v-model.trim=\"form.nome\"\r\n                type=\"text\"\r\n                required\r\n                placeholder=\"Seu nome completo\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"row\">\r\n              <label for=\"cpf\">CPF</label>\r\n              <input\r\n                id=\"cpf\"\r\n                v-model=\"form.cpf\"\r\n                @input=\"form.cpf = maskCpf(form.cpf)\"\r\n                inputmode=\"numeric\"\r\n                required\r\n                placeholder=\"000.000.000-00\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"row\">\r\n              <label for=\"cnpj\">CNPJ</label>\r\n              <input\r\n                id=\"cnpj\"\r\n                v-model=\"form.cnpj\"\r\n                @input=\"form.cnpj = maskCnpj(form.cnpj)\"\r\n                inputmode=\"numeric\"\r\n                required\r\n                placeholder=\"00.000.000/0000-00\"\r\n              />\r\n            </div>\r\n\r\n            <div class=\"row\">\r\n              <label for=\"email\">E-mail corporativo</label>\r\n              <input\r\n                id=\"email\"\r\n                v-model.trim=\"form.email\"\r\n                type=\"email\"\r\n                required\r\n                placeholder=\"voce@empresa.com\"\r\n              />\r\n            </div>\r\n\r\n            <small v-if=\"error\" class=\"error\">{{ error }}</small>\r\n\r\n            <button class=\"submit\" type=\"submit\">\r\n              Quero agendar\r\n            </button>\r\n\r\n            <p class=\"hint\">\r\n              * CPF e CNPJ são obrigatórios para validação.\r\n            </p>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nconst onlyDigits = v => (v || \"\").replace(/\\D/g, \"\");\r\n\r\nexport default {\r\n  name: \"HeroSection\",\r\n  data() {\r\n    return {\r\n      form: {\r\n        nome: \"\",\r\n        cpf: \"\",\r\n        cnpj: \"\",\r\n        email: \"\",\r\n      },\r\n      error: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    maskCpf(v) {\r\n      const d = onlyDigits(v).slice(0, 11);\r\n      return d\r\n        .replace(/^(\\d{3})(\\d)/, \"$1.$2\")\r\n        .replace(/^(\\d{3})\\.(\\d{3})(\\d)/, \"$1.$2.$3\")\r\n        .replace(/^(\\d{3})\\.(\\d{3})\\.(\\d{3})(\\d{1,2})$/, \"$1.$2.$3-$4\");\r\n    },\r\n    maskCnpj(v) {\r\n      const d = onlyDigits(v).slice(0, 14);\r\n      return d\r\n        .replace(/^(\\d{2})(\\d)/, \"$1.$2\")\r\n        .replace(/^(\\d{2})\\.(\\d{3})(\\d)/, \"$1.$2.$3\")\r\n        .replace(/^(\\d{2})\\.(\\d{3})\\.(\\d{3})(\\d)/, \"$1.$2.$3/$4\")\r\n        .replace(\r\n          /^(\\d{2})\\.(\\d{3})\\.(\\d{3})\\/(\\d{4})(\\d{1,2})$/,\r\n          \"$1.$2.$3/$4-$5\"\r\n        );\r\n    },\r\n    onSubmit() {\r\n      const cpfDigits = onlyDigits(this.form.cpf);\r\n      const cnpjDigits = onlyDigits(this.form.cnpj);\r\n\r\n      if (cpfDigits.length !== 11) {\r\n        this.error = \"CPF inválido. Preencha corretamente.\";\r\n        return;\r\n      }\r\n      if (cnpjDigits.length !== 14) {\r\n        this.error = \"CNPJ inválido. Preencha corretamente.\";\r\n        return;\r\n      }\r\n\r\n      this.error = \"\";\r\n      alert(\"Dados enviados com sucesso! Entraremos em contato.\");\r\n\r\n      this.form = { nome: \"\", cpf: \"\", cnpj: \"\", email: \"\" };\r\n    },\r\n  },\r\n};\r\